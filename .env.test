# AI Interview Simulation Platform - Test Environment Variables

# Application
APP_ENV=test
APP_NAME=AI Interview Simulation Platform
DEBUG=true
PORT=8001
HOST=0.0.0.0
LOG_LEVEL=DEBUG
SECRET_KEY=test_secret_key_for_development_only
JWT_SECRET=test_jwt_secret_for_development_only
TOKEN_EXPIRE_MINUTES=60
CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:3000

# Database
# PostgreSQL - Use test databases
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=interview_platform_test
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_URI=postgresql://${POSTGRES_USER}:${POSTGRES_PASSWORD}@${POSTGRES_HOST}:${POSTGRES_PORT}/${POSTGRES_DB}

# MongoDB
MONGODB_URI=mongodb://localhost:27017/interview_platform_test
MONGODB_USER=
MONGODB_PASSWORD=

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_URI=redis://${REDIS_HOST}:${REDIS_PORT}

# AI Services - Use mock services for testing
MOCK_AI_SERVICES=true
OPENAI_API_KEY=sk-test-key
OPENAI_ORGANIZATION=org-test

# Google Cloud
GOOGLE_APPLICATION_CREDENTIALS=test/fixtures/mock-gcp-credentials.json
GOOGLE_CLOUD_PROJECT=test-project

# AWS
AWS_ACCESS_KEY_ID=test_access_key
AWS_SECRET_ACCESS_KEY=test_secret_key
AWS_REGION=us-west-2
AWS_S3_BUCKET=test-bucket

# Frontend
REACT_APP_API_URL=http://localhost:8001
REACT_APP_WS_URL=ws://localhost:8001/ws
REACT_APP_ENVIRONMENT=test 